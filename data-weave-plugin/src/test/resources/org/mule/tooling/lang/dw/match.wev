{
  // Regex Pattern Matching (Can be named or unnamed)
  a: in0.phones map $ match {
    case /\+(\d+)\s\((\d+)\)\s(\d+\-\d+)/ -> { country: $[0], area: $[1], number: $[2] }
    case /\((\d+)\)\s(\d+\-\d+)/ -> { area: $[1], number: $[2] }
    case phone: /\((\d+)\)\s(\d+\-\d+)/ -> { area: phone[1], number: phone[2] }
  },
  // Type Pattern Matching (Can be named or unnamed)
  b: in0.object match {
    case Object -> { object: $ }
    case Number -> { number: $ }
    // This is how you name variables when needed
    case y is Boolean -> { boolean: y }
  },
  // Literal Pattern Matching (Can be named or unnamed)
  c: in0.value match {
    case "Emiliano" -> { string: $ }
    case 123 -> { number: $ }
    // This is how you name variables when needed
    case value: "Mariano" -> { name: value }
  },
  // Boolean Expression Pattern Matching (Always named)
  d: in0.value match {
    case x when x > 30 -> { biggerThan30: x }
    case x when x == 9 -> { nine: x }
  },
  // Default matches
  e: in0.value match {
    case "Emiliano" -> "string"
    case 3.14 -> number
    otherwise -> "1234"
  }
}